{
	"name": "express",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "node dist/index.js",
		"dev": "cross-env NODE_ENV=development nodemon --exec ts-node-esm --files index.ts",
		"build": "tsc",
		"clean": "rimraf dist",
		"typecheck": "tsc --noEmit",
		"lint": "eslint ."
	},
	"nodemonConfig": {
		"watch": [
			"./"
		],
		"ext": "ts",
		"ignore": [
			"dist"
		],
		"exec": "ts-node-esm --files index.ts"
	},
	"dependencies": {
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"mongodb": "^6.8.0",
		"mongoose": "^8.5.2"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.7",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^9.8.0",
		"nodemon": "^3.1.4",
		"typescript": "^5.5.4"
	}
}
